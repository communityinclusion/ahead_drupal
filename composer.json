{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"

        },
        {
           "type": "package",
           "package": {
           "name": "d3/d3",
           "version": "v3.5.17",
           "type": "drupal-library",
           "dist": {
            "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
            "type": "zip"
                }
            }
         },
         {
          "type": "package",
          "package": {
          "name": "components/highlightjs",
          "version": "9.7.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
            "type": "zip"
                   }
                   }
          }

    ],
    "require": {
        "php": ">=5.6",
        "components/highlightjs": "9.7.*",
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.6.5",
        "d3/d3": "3.5.*",
        "drupal/address": "^1.4",
        "drupal/admin_toolbar": "^3.2",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/block_class": "^2.0",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_library": "^2.0",
        "drupal/captcha": "^1.0@beta",
        "drupal/coder": "^8.3.2",
        "drupal/computed_field": "^3.0@alpha",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/core": "^10.1.5",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-dev": "^10.1",
        "drupal/csv_importer": "1.x-dev",
        "drupal/csv_serialization": "^3.0",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^5.0",
        "drupal/ds": "^3.2",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity": "^1.1",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_reference_revisions": "^1.8",
        "drupal/entity_usage": "^2.0",
        "drupal/facets": "^2.0",
        "drupal/feeds": "3.x-dev",
        "drupal/feeds_tamper": "2.x-dev",
        "drupal/field_display_label": "^1.3",
        "drupal/field_group": "3.x-dev",
        "drupal/flag": "^4.0",
        "drupal/fontawesome": "^2.15",
        "drupal/fontyourface": "3.x-dev@dev",
        "drupal/force_password_change": "^2.0",
        "drupal/geofield": "^1.1",
        "drupal/geofield_map": "^2.57",
        "drupal/google_tag": "^1.4",
        "drupal/imce": "^3.0",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jquery_ui_slider": "^2.0",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/leaflet": "^2.1",
        "drupal/mailgun": "^2.0",
        "drupal/mailsystem": "^4.4",
        "drupal/memcache": "^2.0",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.14",
        "drupal/nodeaccess": "^2.0@alpha",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.3",
        "drupal/paragraphs": "^1.9",
        "drupal/pathauto": "^1.6",
        "drupal/profile": "^1.1",
        "drupal/recaptcha": "^3.0",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.28",
        "drupal/search_api_db": "^1.17",
        "drupal/search_api_page": "^1.0",
        "drupal/search_api_saved_searches": "^1.0@alpha",
        "drupal/search_api_solr": "^4.2",
        "drupal/smtp": "^1.0@beta",
        "drupal/subpathauto": "^1.0",
        "drupal/superfish": "^1.3",
        "drupal/tamper": "1.x-dev",
        "drupal/token": "^1.5",
        "drupal/token_custom": "^1.0@alpha",
        "drupal/token_filter": "^2.0",
        "drupal/upgrade_status": "^4.0",
        "drupal/vde_drush": "^2.1",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_bulk_operations": "^4.0@RC",
        "drupal/views_data_export": "^1.3",
        "drupal/workflow": "^1.1",
        "drush/drush": "^12.3",
        "egulias/email-validator": "^3.2",
        "pear/archive_tar": "^1.4.10",
        "smalot/pdfparser": "^0.14.0",
        "solarium/solarium": "^6",
        "symfony/debug": "^4.4",
        "symfony/error-handler": "^6.3",
        "symfony/filesystem": "^6.3",
        "symfony/http-foundation": "^6.3",
        "typo3/phar-stream-wrapper": "^3.1.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
